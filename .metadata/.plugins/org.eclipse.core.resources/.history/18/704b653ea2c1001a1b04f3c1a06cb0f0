package Ch11;

public class C01Array {
	public static void main(String[] args) {
		int[] arr1 = new int[5];
		
		double[] arr2 = new double[7];
		
		float[] arr3;
		
		arr3 = new float[9];
		
		System.out.println("배열의 길이"+arr1.length);
		System.out.println("배열의 길이"+arr2.length);
		System.out.println("배열의 길이"+arr3.length);
	}
}
