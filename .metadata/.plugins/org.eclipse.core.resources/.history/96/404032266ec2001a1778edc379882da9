package Ch12;

public class C08ReservationMain {
	public static void main(String[] args) {
		
		// 객체 생성
		C08Revation Revation01 = new C08Revation();
		C08Revation Revation02 = new C08Revation();
		C08Revation Revation03 = new C08Revation();
		
		// 예약
		Revation01.setReserve("Lee", "010-0000-0000");
		Revation01.getReserve();
		Revation02.setReserve("Kim", "010-0000-0000");
		Revation02.getReserve();
		Revation03.setReserve("Jung", "010-0000-0000");
		Revation03.getReserve();
		
		
	}
}
