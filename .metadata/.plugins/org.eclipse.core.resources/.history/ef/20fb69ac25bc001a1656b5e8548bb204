	public static void main(String[] args) throws IOException {
		int keyCode;
		
		while (true) {
			keyCode = System.in.read();
			System.out.println("keyCode: "+keyCode);
			if (keyCode==113) {
				break;
			} else {

			}
		}

		System.out.println("Á¾·á");
		
	}
}
