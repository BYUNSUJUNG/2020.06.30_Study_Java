package Ch20;
class C08Car {
	
}

public class C08ClassTest {
	public static void main(String[] args) {
		Class cls = Ch20.C08Car.class;
		
		System.out.println(cls.getName()); // 패키지.클래스 명 출력
		System.out.println(cls.getSimpleName()); // 클래스 명만 출력
		System.out.println(cls.getPackage()); // 패키지 명 출력
	}
}
