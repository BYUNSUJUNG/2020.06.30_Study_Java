package Ch20;

public class C11WrapperTest {
	public static void main(String[] args) {
		// 자동 Boxing
		
		Integer obj = 100;
		System.out.println("VALUES: "+obj.intValue());
		
		// 자동 unBoxing
		int value = obj;
		System.out.println("VALUES: "+value);
		
		// 연산시 자동 UnBoxing
		int result = obj + 100;
		System.out.println("VALUES: "+result);
	}
}
