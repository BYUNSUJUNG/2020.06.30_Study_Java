
import java.util.Arrays;
import java.util.Scanner;

public class Statistics2 {

	public static void main(String[] args) {
		
		int[] index = new int[5]; //인덱스의 카운터
		index[1] = 0;
		index[500] = 0;
		index[1050] = 0;
		index[200] = 0;
		index[600] = 0;
		for (int i = 0; i < index.length; i++) {
			System.out.println(index[i]);
		}
		
	}

}