package Ch11;

import java.util.Scanner;

public class C04Array {
	public static void main(String[] args) {
		int[] aaa = {1,2,3};
		int[] bbb = {3,2,1};
		
		// °ª ´ëÀÔ
		for (int i = 0; i < aaa.length; i++) {
			bbb[i]=aaa[i];
		}
		
		System.out.println(bbb[0]);
		System.out.println(bbb[1]);
		System.out.println(bbb[2]);
		
	}
}
