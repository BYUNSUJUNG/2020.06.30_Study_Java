package Ch19;

public class C10Students extends C05People{

	String school;
	
	C10Students(String name,int age,String school)
	{
		super(name,age);
		this.school=school;
	}
	@Override
	public boolean equals(Object obj)
	{
		if(obj instanceof C10Students)
		{
			C10Students tmp=(C10Students)obj;
			return this.school==tmp.school;
		}
		else
		{
			return false;
		}
	}
}
