package Ch24;

import java.util.*;

public class C06Student {
	// 필드
	String name;
	int id;
	
	// 생성자
	C06Student(String name, int age) {
		this.name = name;
		this.id = id;
	}
}
public class C06HashSetTest2 {
	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		
		Set<C06Member> set = new HashSet();
		set.add(new C06Member("홍길동",10101111));
		set.add(new C06Member("이지성",10101122));
		set.add(new C06Member("남산애",10101133));
		set.add(new C06Member("서신애",10101414));
		set.add(new C06Member("금영석",10101524));
		
		
		// 객체 동등비교
		// 반복자
		Iterator<C06Member> iterator = set.iterator();
		ArrayList <C06Member> list = new ArrayList();
		
		while (iterator.hasNext()) {
			list.add(iterator.next());
		}
		
		// 이름 입력
		System.out.print("이름 검색: ");
		String name = scan.nextLine();
		
		list.get(name);


		
	}
}
