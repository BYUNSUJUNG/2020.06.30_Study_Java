package Ch13;

class Parent {
	int x;
	int y;
	public Parent() {
		this.x = 10;
		this.y = 20;
		
	}
	
	void ShowMember() {
		System.out.println(this.x+","+this.y);
	}
	
}

class Son extends Parent {
	int z;
	public Son() {
		this.x = 10;
		this.y = 20;
		this.z = 30;
		
	}
	
	void ShowMember() {
		System.out.println(this.x+","+this.y);
	}
}
public class C03Overriding {
	public static void main(String[] args) {
		Son son = new Son();
		son.ShowMember();
	}
}
