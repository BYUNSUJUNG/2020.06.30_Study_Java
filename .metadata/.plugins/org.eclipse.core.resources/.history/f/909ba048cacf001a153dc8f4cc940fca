package Ch25;

import java.io.*;

public class C06CharStreamEx {
	public static void main(String[] args) {
		try {

			Writer out = new FileWriter("c:/temp/test2.txt");
			char[] array = {'A', 'B', 'C', 'D', 'E'};
			
			out.write(array);
			out.flush();
			out.close();
		} catch(Exception e) {
			
		}
		
	}
}
