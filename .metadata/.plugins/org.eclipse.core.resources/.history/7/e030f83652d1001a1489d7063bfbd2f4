public class C02Thread {
	public static void main(String[] args) {
		
		C02Task task2 = new C02Task();
		Thread th2 = new Thread(task2);
		th2.start();
		
		for (int i = 0; i < 5; i++) {
			System.out.println("Main Thread");
			try {Thread.sleep(500);} catch(Exception e) {};
		}
	}
}
