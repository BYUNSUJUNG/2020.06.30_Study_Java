package Ch19;

public class C10Main {

	public static void main(String[] args) {
		C10Students kim=new C10Students("ss",24,"대구대학교");
		C10Students Lee=new C10Students("이승찬",22,"구미대학교");
		C10Students Jung=new C10Students("정우균",24,"대구대학교");
		
		System.out.println(kim.equals(Lee));
		System.out.println(kim.equals(Jung));
	}

}
