package Ch09;

public class C03PointMain {
	public static void main(String[] args) {
		C03Point pt01 = new C03Point();
		pt01.setx(10);
		pt01.sety(20);
		
		System.out.println(pt01.getx()+","+pt01.gety());
		
		
		C03Point pt02 = new C03Point();
		pt02.setx(60);
		pt02.sety(70);
		
		System.out.println(pt02.getx()+","+pt02.gety());
		
		
	}
}
