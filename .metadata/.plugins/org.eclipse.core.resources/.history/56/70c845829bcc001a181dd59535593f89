package Ch24;

import java.util.*;

public class C01ArrayListTest {
	public static void main(String[] args) {
		List <String> list = new ArrayList<String>();
		list.add("java");
		list.add("JDBC");
		list.add("Servlet/JSP");

		for (int i = 0; i < list.size(); i++) {
			String str = list.get(i);
			System.out.println(i+": "+str.length());
		}
	}
}
