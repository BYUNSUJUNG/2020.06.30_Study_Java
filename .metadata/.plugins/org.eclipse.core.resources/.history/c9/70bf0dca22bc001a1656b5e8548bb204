package Ch03;

public class C015SystemInRead {
	public static void main(String[] args) {
		int keyCode;
		
		keyCode = System.in.read();
		System.out.println("KeyCode: "+keyCode);
		
		keyCode = System.in.read();
		System.out.println("KeyCode: "+keyCode);
		
		keyCode = System.in.read();
		System.out.println("KeyCode: "+keyCode);
		
		
	}
}
