package Ch25;

import java.io.FileOutputStream;
import java.io.OutputStream;

// Array, Stream
public class C02StreamEx {
	public static void main(String[] args) {
		try {
			OutputStream out = new FileOutputStream("C:/temp/test2.db");
			byte[] array = {10,20,30};
			
			out.write(array);
			out.flush();
			out.close();
		} catch(Exception e) {
			
		}
	}
}
