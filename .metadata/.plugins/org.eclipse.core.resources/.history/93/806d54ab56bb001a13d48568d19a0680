package Ch01;

public class Ch03Formatting {

	public static void main(String[] args) {
		// 서식문자
		System.out.printf("%d\n",10);
		System.out.printf("%f\n",10.1);
		System.out.printf("%c\n",'A');
		System.out.printf("%s\n","This is String Test");
		
		System.out.printf("%d + %d = %d\n",5,12,5+12);
		System.out.printf("%d * %d = %d\n",7,3,7*3);
	}

}
