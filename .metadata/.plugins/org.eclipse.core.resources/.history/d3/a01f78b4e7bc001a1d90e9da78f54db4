package Ch05;

public class C01ArithBasic {

	public static void main(String[] args) {
		// 기본 산술 연산자
		int a = 10 , b = 20, c =0;
		System.out.println("a + b ="+(a+b));
		System.out.println("a - b ="+(a-b));
		System.out.println("a * b ="+(a*b));
		System.out.println("a / b ="+(a/b));
		System.out.println("a %% b ="+(a%b));
		
		// 복합 대입 연산자
		a += 10;
		b -= 10;
		System.out.println("a="+a);
		System.out.println("b="+b);

	}

}
