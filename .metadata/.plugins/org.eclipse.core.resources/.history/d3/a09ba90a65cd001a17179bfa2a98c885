package Ch24;

public class C06Student {
	// 필드
	String name;
	int id;
	
	// 생성자
	C06Student(String name, int age) {
		this.name = name;
		this.id = id;
	}
	
	public boolean equals(Object obj) {
		if(obj instanceof C06Student) { // 상속관계 확인
			C06Student tmp = (C06Student)obj; // downCasting
			if(name.equals(tmp.name)&&(id==tmp.id)) {
				return true;
			} else {
				return false;
			}
		}
		return false;
	}
}
