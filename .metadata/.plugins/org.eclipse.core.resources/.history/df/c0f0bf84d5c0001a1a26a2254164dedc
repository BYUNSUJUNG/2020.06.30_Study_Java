package Ch09;

// 생성자 오버로드
public class C02ConstOverMain {
	// 매개변수
	int x;
	int y;
	int z;
	
	// 디폴트 생성자
	public C02ConstOverMain() {
		
	}
	
	// 오버로딩된 생성자
	public C02ConstOverMain(int a, int b) {
		this.x = a;
		this.y = b;
	}
	
	// 오버로딩된 생성자
	public C02ConstOverMain(int a, int b, int c) {
		this.x = a;
		this.y = b;
		this.z = c;
	}
	
	public void ShowPoint() {
		System.out.println(this.x+","+this.y+","+this.z);
	}
}
